{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=UrlShortener.db",
    "Redis": "localhost:6379",
    "RabbitMQ": "localhost"
  },
  "UrlShortening": {
    "DefaultShortCodeLength": 6,
    "MaxRetries": 5,
    "CacheExpirationHours": 24
  }
}